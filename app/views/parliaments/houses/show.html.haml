- title("#{@house.name} - #{@parliament.date_range(date_format: '%Y')} #{t('parliament')}")
%section.section--primary
  .container
    %h1
      %span= @house.name
      = link_to("#{@parliament.date_range(date_format: '%Y')} #{t('parliament').capitalize}", parliament_path(@parliament.graph_id))

%section#content{ tabindex: "0" }
  .container
    %ul.list--block
      %li
        .card
          .card__details
            %h2= link_to(t('mps_and_lords'), parliament_house_members_path(@parliament.graph_id, @house.graph_id))
      %li
        .card
          .card__details
            %h2= link_to(t('party_plural').capitalize, parliament_house_parties_path(@parliament.graph_id, @house.graph_id))
