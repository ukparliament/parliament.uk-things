- title("#{@party.name} - #{@parliament.date_range(date_format: '%Y')} #{t('parliament')}")
%section.section--primary
  .container
    %h1
      %span= @party.name
      %span= link_to("#{@parliament.date_range(date_format: '%Y')} #{t('parliament').capitalize}", parliament_path(@parliament.graph_id))
%section#content{ tabindex: "0" }
  .container
    %ul.list--block
      %li
        .card
          .card__details
            %h2= link_to(t('mps_and_lords'), parliament_party_members_path(@parliament.graph_id, @party.graph_id))
